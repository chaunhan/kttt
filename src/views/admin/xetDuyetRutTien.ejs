<%- include('../layouts/headeradmin'); -%>
<div>
<h2>Update Trạng Thái Chuyển Khoản</h2>
<form action="/admin/lenh-rut-tien/xuly/<%= request._id %>" method="POST">

  <div class="mb-3">
    <input type="hidden" name="email" value="<%= request.email %>" />
  </div>

  <div class="mb-3">
    <input type="hidden" value="<%= request.tienrut %>" class="form-control" name="tienrut" />
  </div>

  <div class="mb-3">
    <input type="hidden" value="<%= user.tien %>" class="form-control" name="tien" />
  </div>

  <div class="mb-3">
    <label class="form-label">Tiền Muốn Rút: <%= tienrut %></label>
    <input type="hidden" value="<%= tienrut %>" class="form-control" name="tienrut1" />
  </div>

  <div class="mb-3">
    <label class="form-label">Tiền Hiện có: <%= tien %></label>
    <input type="hidden" value="<%= tien %>" class="form-control" name="tien1" />
  </div>

  <div class="mb-3">
    <label class="form-label">Trạng Thái</label>
    <input type="input" value="<%= request.isCheck %>" class="form-control" name="isCheck" />
  </div>


  <button type="submit" class="btn btn-primary">Update</button>
</form>

</div>