        <script>
            $('#modal_cropper').on('shown.bs.modal', function() {
                var img = $('#modal_cropper_img');
                img.cropper('destroy');
                img.cropper({
                    aspectRatio: img.data('width') / img.data('height'),
                    responsive: true,
                    center: true,
                    cropBoxResizable: false,
                    viewMode: 0,
                    dragMode: 'move',
                    zoomable: true,
                    minCropBoxWidth: img.data('width'),
                    minCropBoxHeight: img.data('height')
                });
            });
            $('#crop_now').on('click', function() {
                $('#page-loader').addClass('show');
                var img = $('#modal_cropper_img');
                var src = img.attr('src');
                var index = src.lastIndexOf("/") + 1;
                var filename = src.substr(index);
                var lang = img.data('lang');
                var name = img.data('name');
                var canvas = img.cropper('getCroppedCanvas');
                var ajax_cropped_img = canvas.toDataURL();
                $.post('/vi/ajax/cropper/', {
                    'ajax_crop_img': src,
                    'ajax_cropped_img': ajax_cropped_img,
                    'width': img.data('width'),
                    'height': img.data('height')
                }, function(data) {
                    if (!data) {
                        alert('Có lỗi, vui lòng tải lại trang và thử lại');
                    } else {
                        $('.thumbnail_zoom[data-name="' + name + '"][data-lang="' + lang + '"]').removeClass('d-none');
                        $('.ajax_cropper_img[data-name="' + name + '"][data-lang="' + lang + '"]').attr('src', data);
                        $('.thumbnail_zoom[data-name="' + name + '"][data-lang="' + lang + '"]').attr('href', data);
                        if ($('input[name="' + name + '"][data-lang="' + lang + '"]').length > 0 && lang == '') {
                            $('input[name="' + name + '"][data-lang="' + lang + '"]').val(data);
                        } else if ($('input[name="' + name + '[' + lang + ']"]').length > 0 && lang != '') {
                            $('input[name="' + name + '[' + lang + ']"]').val(data);
                        }
                        $('#modal_cropper').modal('hide');
                    }
                    $('#page-loader').removeClass('show');
                });
            });
        </script>
