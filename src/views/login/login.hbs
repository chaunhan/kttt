        <script src="./Đăng nhập_files/toastr.min.js.tải xuống"></script>
        <script>
            function scrollToTop() {
                window.scrollTo({
                    top: 0,
                    behavior: 'smooth'
                });
            }
            $(function() {
                toastr.options = {
                    "closeButton": false,
                    "debug": false,
                    "newestOnTop": false,
                    "progressBar": false,
                    "positionClass": "toast-top-right",
                    "preventDuplicates": true,
                    "onclick": null,
                    "showDuration": "300",
                    "hideDuration": "1000",
                    "timeOut": "5000",
                    "extendedTimeOut": "1000",
                    "showEasing": "swing",
                    "hideEasing": "linear",
                    "showMethod": "fadeIn",
                    "hideMethod": "fadeOut"
                };
                $('.add_cart').click(function() {
                    var id = $(this).data('course');
                    $.post('/vi/cart/ajax/', {
                        type: 'add_cart',
                        course: id
                    }, function(rs) {
                        if (rs.status == 1) {
                            if ($('.cart-detail').find('.empty_cart').length != 0) {
                                $('.cart-detail').html(rs.content);
                            } else {
                                $('.cart-detail').append(rs.content);
                            }
                            $('.qty_cart').html(rs.message);
                            $('.bi').addClass('show');
                            Command: toastr["success"]('Thêm vào giỏ hàng thành công!');
                            scrollToTop();
                        }
                    });
                });
                $('.buy_now').click(function() {
                    var id = $(this).data('course');
                    $.post('/vi/buy/ajax/', {
                        type: 'buy_now',
                        course: id
                    }, function(rs) {
                        if (rs.status == 1) {
                            if ($('.cart-detail').find('.empty_cart').length != 0) {
                                $('.cart-detail').html(rs.content);
                            } else {
                                $('.cart-detail').append(rs.content);
                            }
                            $('.qty_cart').html(rs.message);
                            $('.bi').addClass('show');
                            Command: toastr["success"]('Thêm vào giỏ hàng thành công!');
                            window.location.href = '/vi/cart/';
                            scrollToTop();
                        }
                    });
                });
            });
            $(function() {
                $('.remove_cart').click(function() {
                    var self = this;
                    var id = $(this).data('id');
                    $.post('/vi/cart/ajax/delete/', {
                        'remove': id
                    }, function(rs) {
                        if ($('.cart_item').find('tr').length != 1) {
                            $(self).parents('tr').remove();
                        } else {
                            $('.cart_item').html('<tr class="table-danger"><td colspan="3" class="text-danger font-w700 text-center">Chưa có khóa học nào</td></tr>');
                        }
                        $('.cart_total').html(rs);
                        Command: toastr["warning"]('Xóa khóa học thành công');
                        location.reload();
                    });
                });
            });
        </script>
